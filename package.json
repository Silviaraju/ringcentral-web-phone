{
  "name": "ringcentral-web-phone",
  "version": "0.9.0",
  "scripts": {
    "test": "npm run test:ut && npm run test:e2e",
    "test:coverage": "cat .coverage/lcov.info | coveralls -v",
    "test:e2e": "jest --config jest.config.e2e.js --runInBand",
    "test:ut": "jest --config jest.config.ut.js",
    "build": "npm run build:tsc && npm run build:webpack",
    "build:tsc": "tsc",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js --progress --color",
    "start": "webpack-dev-server --config webpack.config.js --progress --color",
    "server": "http-server --port ${PORT:-8080}",
    "watch": "npm-run-all --print-label --parallel \"build:** -- --watch\"",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint --fix",
    "lint:all": "npm run lint \"src/**/*.ts\" \"demo/**/*.js\"",
    "lint:staged": "lint-staged",
    "docs:webphone": "typedoc --options ./webphone-typedoc.json",
    "docs:sipjs": "typedoc --options ./sipjs-typedoc.json",
    "docs": "npm run docs:webphone && npm run docs:sipjs"
  },
  "dependencies": {
    "sip.js": "0.20.0"
  },
  "devDependencies": {
    "@faker-js/faker": "6.0.0",
    "@types/expect-puppeteer": "3.3.1",
    "@types/jest": "24.0.15",
    "@types/jest-environment-puppeteer": "4.0.0",
    "@types/node": "12.0.8",
    "@typescript-eslint/parser": "5.15.0",
    "bootstrap": "3.4.1",
    "copy-webpack-plugin": "10.2.4",
    "coveralls": "3.0.4",
    "cross-env": "5.2.0",
    "dotenv": "8.0.0",
    "eslint": "8.11.0",
    "eslint-config-ringcentral-typescript": "7.0.3",
    "html-webpack-plugin": "5.5.0",
    "http-server": "0.11.1",
    "husky": "2.4.1",
    "jest": "24.8.0",
    "jest-puppeteer": "4.2.0",
    "jquery": "3.4.1",
    "lint-staged": "8.2.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "prettier-eslint": "13.0.0",
    "process": "0.11.10",
    "puppeteer": "1.18.0",
    "querystring-es3": "0.2.1",
    "ringcentral": "3.2.2",
    "terser-webpack-plugin": "5.3.1",
    "ts-jest": "24.0.2",
    "ts-loader": "6.0.3",
    "typedoc": "0.22.13",
    "typedoc-plugin-markdown": "3.11.14",
    "typedoc-plugin-merge-modules": "3.1.0",
    "typedoc-plugin-rename-defaults": "0.4.0",
    "typescript": "4.6.2",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  },
  "preferGlobal": false,
  "private": false,
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "author": {
    "name": "RingCentral, Inc.",
    "email": "devsupport@ringcentral.com"
  },
  "contributors": [
    {
      "name": "Kirill Konshin"
    },
    {
      "name": "Elias Sun"
    },
    {
      "name": "Vyshakh Babji"
    },
    {
      "name": "Yatin Gera"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/ringcentral/ringcentral-web-phone.git"
  },
  "bugs": {
    "url": "https://github.com/ringcentral/ringcentral-web-phone/issues"
  },
  "homepage": "https://github.com/ringcentral/ringcentral-web-phone",
  "engines": {
    "node": ">=0.10.36"
  },
  "license": "MIT"
}
